<viewdata model="FubuMovies.Api.MovieNewViewModel" />
<content:title>
  Add Movie
</content:title>
<h1>Add Movie</h1>
<Form model="MovieCreateInputModel">
  <div class="row">
    <LabelFor property="Name" />
    <InputFor property="Name" />
  </div>
  <div class="row">
    <LabelFor property="Description" />
    <InputFor property="Description" />
  </div>
  <div class="row row-submit">
    <button type="submit">
      <span>Add Movie</span>
    </button>
  </div>
</Form>